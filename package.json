{
  "name": "reactive-scriptable-components",
  "version": "0.0.1",
  "description": "light-weight reactive scriptable web components",
  "type": "module",
  "main": "./dist/reactive-scriptable-components.js",
  "module": "./dist/reactive-scriptable-components.esm.js",
  "svelte": "./dist/reactive-scriptable-components.esm.js",
  "types": "./dist/reactive-scriptable-components.d.ts",
  "exports": {
    ".": {
      "require": "./dist/reactive-scriptable-components.js",
      "import": "./dist/reactive-scriptable-components.esm.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rimraf dist && rollup -c rollup.config.js && tsc && mv src/*.d.ts dist && rm src/*.js* && rm -r dist/src",
    "agadoo": "agadoo",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rozek/reactive-scriptable-components.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "reactive",
    "preact",
    "web-components",
    "custom-elements",
    "htm",
    "scriptable",
    "hyperactiv"
  ],
  "author": "Andreas Rozek <a.rozek@gmx.de> (https://www.rozek.de/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rozek/reactive-scriptable-components/issues"
  },
  "homepage": "https://github.com/rozek/reactive-scriptable-components#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.1",
    "agadoo": "^2.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.1",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.6.2",
    "typescript": "^4.3.3"
  },
  "dependencies": {
    "htm": "^3.1.1",
    "hyperactiv": "^0.11.3",
    "javascript-interface-library": "^1.0.5",
    "preact": "^10.19.3"
  }
}
